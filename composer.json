{
  "name": "vi-tech/dto-bundle",
  "description": "DTO resolver for symfony projects",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Temirkhan",
      "email": "temirkhan@nasukhov.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "ViTech\\DataObjectBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\ViTech\\DataObjectBundle\\": "test/"
    }
